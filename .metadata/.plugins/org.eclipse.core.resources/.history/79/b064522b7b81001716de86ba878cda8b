package com.tutorials.session1;

public class Triangle {
	
	//Step 4- Add a property and configure it
	public String type = null;
	
	public String getType() {
		return type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public void draw()
	{
		System.out.println("Draw a triangle");
	}

}
